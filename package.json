{
  "name": "api-ts-express",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/main.ts",
    "dev:win": "set NODE_ENV=development&& tsx watch src/main.ts ",
      "dev:win:pretty": "set NODE_ENV=development&& tsx watch src/main.ts | npx pino-pretty --singleLine --colorize --translateTime \"SYS:isoTime\" --ignore pid,hostname,service,env,version",
    "build": "tsc -p tsconfig.json",
    "start": "NODE_ENV=production node dist/main.js",
    "start:win": "set NODE_ENV=production&& node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:local": "jest --coverage --verbose --detectOpenHandles --runInBand --coverageReporters=text --coverageReporters=text-summary --coverageReporters=lcov --coverageReporters=html --coverageReporters=json-summary",
    "test:repo": "jest --coverage --coverageReporters=text-summary",
    "lint": "eslint \"src/**/*.{ts,js}\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
